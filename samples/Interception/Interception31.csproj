    <PackageReference Include="Interception.DeadlockDetection" Version="1.2020.6170" />
    <PackageReference Include="Interception.MassTransit" Version="1.2020.6170" />
    <PackageReference Include="Interception.Observers" Version="1.2020.6170" />
    <PackageReference Include="Interception.OpenTracing.Prometheus" Version="1.2020.6170" />
