    <PackageReference Include="Interception.Tracing.Serilog" Version="1.2020.6170" />
    <PackageReference Include="MassTransit.RabbitMQ" Version="6.2.5" />
    <PackageReference Include="Jaeger" Version="0.3.7" />
    <PackageReference Include="Microsoft.Extensions.Caching.StackExchangeRedis" Version="3.1.4" />
    <PackageReference Include="Serilog.Extensions.Hosting" Version="3.0.0" />
